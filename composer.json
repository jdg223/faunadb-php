{
	"name": "m50/faunadb",
	"description": "A library for interacting with FaunaDB and writing FQL.",
	"license": "MIT",
	"keywords": [
		"fauna",
		"faunadb",
		"db",
		"fql"
	],
	"autoload": {
		"psr-4": {
			"FaunaDB\\": "src/"
		},
		"files": [
			"src/autoload.php"
		]
	},
	"require": {
		"php": "^8.0",
		"php-http/httplug": "^2.2",
		"psr/http-message": "^1.0",
		"psr/http-client": "^1.0",
		"php-http/message-factory": "^1.0",
		"php-http/discovery": "^1.14",
		"webmozart/assert": "^1.10"
	},
	"require-dev": {
		"vimeo/psalm": "^4.7",
		"pestphp/pest": "^1.7",
		"php-http/mock-client": "^1.4",
		"php-http/message": "^1.11",
		"guzzlehttp/psr7": "^1.8",
		"friendsofphp/php-cs-fixer": "^3.0",
		"symfony/var-dumper": "^5.3"
	}
}
